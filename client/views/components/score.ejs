<%/* model: { date, time, score, team1, team2 } */%>
<% 
    var hasScore = score !== '-:-';
    var matcheData = date + ' ' + team1 + ' ' + team2;
    var youtubeHref = 'https://www.youtube.com/results?search_query=highlights ' + matcheData + ' ' + score;
    var googleHref = 'https://www.google.fr/search?q=' + matcheData;
%>
<% if (hasScore) { %>
    <a href="<%= youtubeHref %>" target="_blank" rel="nofollow noopener noreferrer">
        <span title="Score <%= score %>">
            <%= score %>
        </span>
    </a>
<% } else if (time) { %>
    <a href="<%= googleHref %>" target="_blank" rel="nofollow noopener noreferrer">
        <span class="time" title="Time <%= time %>" data-time="<%= time %>">
            <%= time %>
        </span>
    </a>
<% } else { %>
    <a href="<%= googleHref %>" target="_blank" rel="nofollow noopener noreferrer">
        <span>-:-</span>
    </a>
<% } %>
